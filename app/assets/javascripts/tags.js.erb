$(function() {
  $('.suggest-tag').magicSuggest({
    required: true,
    useTabKey: true,
    useCommaKey: true,
    useZebraStyle: true,
    inputCfg: {"id":'tag-suggest-input'},
    data: <%= Tag.name_list %>
  });

  $('.question').on('click', '.question-details .edit', function(ev) {
    $('.suggest-tag').magicSuggest({
      required: true,
      useTabKey: true,
      useCommaKey: true,
      useZebraStyle: true,
      inputCfg: {"id":'tag-suggest-input'},
      data: <%= Tag.name_list %>
    });
  });
});
