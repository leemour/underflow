  var ms = $('.suggest-tag').magicSuggest({
    allowFreeEntries: true,
    required: true,
    useTabKey: true,
    useCommaKey: true,
    resultAsString: true,
    useZebraStyle: true,
    inputCfg: {"id":'tag-suggest-input'},
    data: <%= Tag.name_list %>,
    value: [1,3]
